{
  "name": "csm_setup",
  "description": "CSM Setup Command",
  "sub_commands": [
    {
      "name": "init",
      "description": "Initialize CSM",
      "need_confirmation": false,
      "permissions_tag": "update",
      "args": [
        {
          "flag": "--config",
          "dest": "config_url",
          "type": "str",
          "help": "Config Store URL e.g <type>://<path>."
        },
        {
          "flag": "-p",
          "dest": "Product",
          "help": "Product name",
          "default": "cortx",
          "type": "str",
          "choices": [
            "cortx"
          ],
          "nargs": "?"
        },
        {
          "flag": "-f",
          "action": "store_true",
          "default": false,
          "help": "force"
        },
        {
          "flag": "-c",
          "dest": "Component",
          "help": "Component name",
          "default": "all",
          "type": "str",
          "choices": [
            "backend",
            "frontend",
            "all"
          ],
          "nargs": "?"
        },
        {
          "flag": "args",
          "default": [],
          "suppress_help": true,
          "nargs": "?"
        }
      ],
      "comm": {
        "type": "direct",
        "target": "csm.conf.init",
        "method": "execute",
        "class": "Init",
        "is_static": false
