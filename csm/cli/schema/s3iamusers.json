{
  "name": "s3iamusers",
  "description": "Create Delete or Display S3 IAM User",
  "sub_commands": [
    {
      "name": "show",
      "description": "Displays S3 iam Users On the cli for logged in account",
      "need_confirmation": false,
      "permissions_tag": "list",
      "args": [
        {
          "flag": "args",
          "default": [],
          "suppress_help": true,
          "nargs": "?",
          "help": ""
        },
        {
          "flag": "-f",
          "dest": "format",
          "help": "Format of Output",
          "default": "table",
          "type": "str",
          "choices": [
            "table",
            "xml",
            "json"
          ]
        }
      ],
      "comm": {
        "type": "rest",
        "method": "get",
        "target": "/{version}/iam_users",
        "version": "v2"
      },
      "output": {
        "table": {
          "headers": {
            "user_name": "User Name",
            "user_id": "User ID",
            "arn": "ARN"
          },
          "filters": "iam_users"
        }
      }
    },
    {
      "name": "create",
      "description": "Create a new S3 IAM user in Logged In S3 Account.",
      "need_confirmation": true,
      "permissions_tag": "create",
      "args": [
        {
          "flag": "user_name",
          "type": "str",
          "help": "Name to be given to S3 IAM user",
          "json": true
        },
        {
          "flag": "-passwd",
          "dest": "password",
          "type": "str",
          "type_method": "Terminal.get_password",
          "type_target": "cortx.utils.cli_framework.terminal",
          "help": "",
          "default": "",
          "suppress_help": true,
          "json": true
        },
        {
          "flag": "args",
          "default": [],
          "suppress_help": true,
          "nargs": "?",
          "help": ""
        },
        {
            "flag": "-f",
            "dest": "format",
            "help": "Format of Output",
            "default": "table",
            "type": "str",
            "choices": [
              "table",
              "xml",
              "json"
            ],
            "suppress_help": true
          }
      ],
      "comm": {
        "type": "rest",
        "method": "post",
        "target": "/{version}/iam_users",
        "version": "v2"
      },
      "output": {
        "table": {
          "headers": {
            "user_name": "User Name",
            "user_id": "User ID",
            "arn": "ARN"
          }
        }
      }
    },
    {
      "name": "password",
      "description": "Change/Reset password for IAM user.",
      "need_confirmation": true,
      "permissions_tag": "update",
      "args": [
        {
          "flag": "user_name",
          "type": "str",
          "help": "Name of IAM user whose password needs to be reset.",
          "json": true
        },
        {
          "flag": "args",
          "default": [],
          "suppress_help": true,
          "nargs": "?",
          "help": ""
        },
        {
          "flag": "-p",
          "dest": "password",
          "type": "str",
          "type_method": "Terminal.get_password",
          "type_target": "cortx.utils.cli_framework.terminal",
          "help": "",
          "default": "",
          "suppress_help": true,
          "json": true
        },
        {
          "flag": "-f",
          "dest": "format",
          "help": "Format of Output",
          "default": "table",
          "type": "str",
          "choices": [
            "table",
            "xml",
            "json"
          ],
          "suppress_help": true
        }
      ],
      "comm": {
        "type": "rest",
        "method": "patch",
        "target": "/{version}/iam_users/{user_name}",
        "version": "v2"
      },
      "output": {
        "table":{
          "headers":{
            "user_name": "User Name"
          }
        }
      }
    },
    {
      "name": "delete",
      "description": "Deletes the given IAM User in logged in  S3 account",
      "need_confirmation": true,
      "permissions_tag": "delete",
      "args": [
        {
          "flag": "user_name",
          "type": "str",
          "help": "Name of the Iam User to be Deleted."
        },
        {
          "flag": "args",
          "default": [],
          "suppress_help": true,
          "nargs": "?",
          "help": ""
        }
      ],
      "comm": {
        "type": "rest",
        "method": "delete",
        "target": "/{version}/iam_users/{user_name}",
        "version": "v2"
      },
      "output": {
        "success": "IAM User Deleted."
      }
    }
  ]
}
